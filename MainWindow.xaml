<Window x:Class="FinalyDem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalyDem"
        mc:Ignorable="d"
        Title="Регистрация" Height="450" Width="850" MinWidth="850" MinHeight="450">
    <Grid Background="Coral">
        
        <TextBox x:Name="Login_Reg" Height="35" Margin="321,95,321,289" TextWrapping="Wrap" Width="200"/>
        <PasswordBox x:Name="Password1_Reg" Height="35" Width="200" Margin="321,163,321,221"/>
        <PasswordBox x:Name="Password2_Reg" Height="35" Width="200" Margin="321,233,321,151"/>

        <Button x:Name="Reg_Button" Content="Регистрация" Height="35" Width="200" Margin="321,291,321,93" Click="Reg_Button_Click"/>
        <Button x:Name="Auton_Button" Content="Есть аккаунт" Height="35" Width="200" Margin="321,342,321,42" Click="Auton_Button_Click"/>
        
        <Label Content="Логин" Margin="321,67,421,322" Width="100" Height="30"/>
        <Label Content="Пароль" Margin="321,135,421,254" Width="100" Height="30"/>
        <Label Content="Повторите пароль" Margin="321,203,404,186" Width="120" Height="30"/>

    </Grid>
</Window>
